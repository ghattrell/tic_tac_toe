<h1>Listing moves</h1>

<table>
  <tr>
    <th>Player</th>
    <th>Symbol</th>
    <th>Cell chosen</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @moves.each do |move| %>
  <tr>
    <td><%= move.player_id %></td>
    <td><%= move.symbol %></td>
    <td><%= move.cell_chosen %></td>
    <td><%= link_to 'Show', [@move.game, @move] %></td>
    <td><%= link_to 'Edit', edit_game_move_path(@move.game, move) %></td>
    <td><%= link_to 'Destroy', move, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Move', new_game_move_path %>
